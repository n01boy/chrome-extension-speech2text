{
  "name": "speech2text-google-meet",
  "version": "1.0",
  "description": "speech2text on google meet",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["https://meet.google.com/*"],
    "js": ["main.js"],
    "run_at": "document_start"
  }],
  "host_permissions": ["https://meet.google.com/*"],
}
